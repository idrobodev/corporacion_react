{
  "name": "fundacion-todo-por-un-alma",
  "type": "dockerfile",
  "dockerfile": "./Dockerfile",
  "buildContext": ".",
  "port": 8080,
  "healthCheck": {
    "path": "/health",
    "interval": 30,
    "timeout": 3,
    "retries": 3
  },
  "env": {
    "NODE_ENV": "production",
    "REACT_APP_AUTH_API_BASE_URL": "https://todoporunalma.org/auth",
    "REACT_APP_DASHBOARD_API_BASE_URL": "https://todoporunalma.org/dashboard",
    "REACT_APP_API_BASE_URL": "https://todoporunalma.org/dashboard",
    "REACT_APP_FORMATOS_API_URL": "https://todoporunalma.org/formatos",
    "REACT_APP_CORS_ORIGINS": "https://todoporunalma.org,https://www.todoporunalma.org"
  }
}
