{
    "name": "fundacion-todo-por-un-alma",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "scripts": {
        "start": "cross-env BROWSER=none PORT=3001 FAST_REFRESH=true craco start",
        "build": "cross-env NODE_ENV=production FAST_REFRESH=false craco build",
        "test": "craco test",
        "eject": "react-scripts eject",
        "analyze": "node scripts/bundle-analysis.js",
        "analyze:interactive": "node scripts/bundle-analysis.js interactive",
        "analyze:report": "node scripts/bundle-analysis.js report",
        "analyze:stats": "node scripts/bundle-analysis.js stats",
        "analyze:compare": "node scripts/bundle-analysis.js compare",
        "build:analyze": "cross-env ANALYZE_BUNDLE=true npm run build",
        "build:stats": "cross-env GENERATE_STATS=true npm run build",
        "docker:build": "docker build -t fundacion-todo-por-un-alma .",
        "docker:run": "docker run -p 3001:80 fundacion-todo-por-un-alma",
        "docker:up": "docker-compose up --build",
        "docker:down": "docker-compose down",
        "docker:logs": "docker-compose logs -f frontend"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.899.0",
        "@aws-sdk/s3-request-presigner": "^3.899.0",
        "@babel/core": "7.16.0",
        "@craco/craco": "^7.1.0",
        "@hookform/resolvers": "^5.2.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.5.1",
        "aos": "^2.3.4",
        "axios": "^0.24.0",
        "chart.js": "^3.9.1",
        "date-fns": "^4.1.0",
        "init": "^0.1.2",
        "jspdf": "^3.0.3",
        "leaflet": "^1.9.4",
        "react": "^18.3.1",
        "react-bootstrap": "^2.10.10",
        "react-chartjs-2": "^4.3.1",
        "react-dom": "^18.3.1",
        "react-dropzone": "^14.3.8",
        "react-helmet": "^6.1.0",
        "react-helmet-async": "^2.0.5",
        "react-hook-form": "^7.62.0",
        "react-icons": "^5.5.0",
        "react-leaflet": "^4.2.1",
        "react-router-dom": "^5.3.0",
        "react-scripts": "5.0.1",
        "react-youtube": "^10.1.0",
        "uuid": "^13.0.0",
        "web-vitals": "^1.0.1",
        "yup": "^1.7.0"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.16.0",
        "@babel/plugin-transform-runtime": "^7.16.0",
        "@babel/preset-env": "^7.16.0",
        "@babel/preset-react": "^7.16.0",
        "@babel/runtime": "^7.16.0",
        "autoprefixer": "^10.4.0",
        "core-js": "^3.45.1",
        "cross-env": "^7.0.3",
        "postcss": "^8.4.0",
        "react-refresh": "^0.18.0",
        "tailwindcss": "^3.4.0",
        "webpack-bundle-analyzer": "^4.10.2"
    }
}